{
  "name": "@quicksend/transmit",
  "version": "1.0.2",
  "description": "An alternative to multer that handle multipart/form-data and cleans up after itself",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .js,.ts",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "jest",
    "version": "npm run format && git add -A src"
  },
  "engines": {
    "node": ">=15.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quicksend/transmit.git"
  },
  "keywords": [
    "form",
    "post",
    "multipart",
    "form-data",
    "formdata"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quicksend/transmit/issues"
  },
  "homepage": "https://github.com/quicksend/transmit#readme",
  "dependencies": {
    "busboy": "^0.3.1"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
